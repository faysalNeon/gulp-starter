{
    "name": "HTML Template Generator",
    "version": "1.0.0",
    "favicon":"favicon.ico",
    "description": "Template Generator",
    "homepage": "https://github.com/faysalNeon/gulp-starter#readme",
    "author": {
        "name": "Faysal Neon",
        "link": "https://faysalneon.github.io",
        "blog": "https://faysalneon.blogspot.com"
    },
    "app": {
        "dist": "app",
        "src": "resources",
        "images": {
            "src": "resources/images/**/*",
            "dist": "app/assets"
        },
        "template": {
            "src": "resources/template/**/*.html",
            "base": "resources/includes",
            "dist": "app"
        },
        "styles": {
            "src": "resources/styles/**/*.scss",
            "dist": "app/assets/styles"
        },
        "scripts": {
            "src": "resources/scripts/**/*.js",
            "dist": "app/assets/scripts"
        }
    },
    "styles": [
        "node_modules/bootstrap/scss/bootstrap.scss"
    ],
    "scripts": [
        "node_modules/jquery/dist/jquery.min.js",
        "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js"
    ],
    "keywords": [
        "template",
        "generator",
        "gulp-cli",
        "node",
        "npm",
        "html",
        "scss",
        "css"
    ]
}